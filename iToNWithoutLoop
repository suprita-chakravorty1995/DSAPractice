public class Solution {
    public static int[] printNos(int x) {
        int[] nums = new int[x];
        printNums(1,x,nums);
        return nums;
    }

    private static void printNums(int i,int n, int[] nums)
    {
      if(i>n)
      {
          return;
      }

      nums[i-1] = i;
      printNums(i+1,n,nums);
    }
}
